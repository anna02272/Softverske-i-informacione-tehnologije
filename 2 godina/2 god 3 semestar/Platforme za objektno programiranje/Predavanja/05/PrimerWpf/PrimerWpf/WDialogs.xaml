<Window x:Class="PrimerWpf.WDialogs"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="WDialogs" Height="300" Width="300">
    <Grid>
        <Button Content="MessageBox" Height="23" HorizontalAlignment="Left" Margin="12,12,0,0" Name="btnMessage" VerticalAlignment="Top" Width="75" Click="btnMessage_Click" />
        <Button Content="Open File" Height="23" HorizontalAlignment="Left" Margin="12,49,0,0" Name="btnOpenFile" VerticalAlignment="Top" Width="75" Click="btnOpenFile_Click" />
        <Button Content="Save File" Height="23" HorizontalAlignment="Left" Margin="12,92,0,0" Name="btnSaveFile" VerticalAlignment="Top" Width="75" Click="btnSaveFile_Click" />
        <Button Content="Print" Height="23" HorizontalAlignment="Left" Margin="12,133,0,0" Name="btnPrint" VerticalAlignment="Top" Width="75" Click="btnPrint_Click" />
    </Grid>
</Window>
